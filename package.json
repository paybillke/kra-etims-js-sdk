{
  "name": "@paybilldev/kra-etims-sdk",
  "version": "1.0.3",
  "description": "A production-ready Node SDK for integrating with the Kenya Revenue Authority (KRA) eTIMS OSCU (Online Sales Control Unit) and VSCU (Virtual Sales Control Unit) API. Built to match the official Postman collection specifications with strict header compliance, token management, and comprehensive validation.",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "keywords": [
    "kra",
    "etims",
    "sdk",
    "vscu",
    "oscu"
  ],
  "author": {
    "name": "Paybill Kenya Limited",
    "email": "support@paybill.ke"
  },
  "scripts": {
    "build": "rimraf dist types && tsc --build tsconfig.build.json",
    "clean": "rimraf node_modules dist .turbo coverage",
    "typecheck": "tsc --project tsconfig.build.json",
    "test": "NODE_ENV=test vitest",
    "cov": "NODE_ENV=test vitest --coverage",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paybillke/kra-etims-js-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "joi": "^18.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "20.17.10",
    "@vitest/coverage-istanbul": "^3.2.4",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fs-extra": "11.3.3",
    "rimraf": "6",
    "ts-node": "10.9.2",
    "typescript": "^5.1.3",
    "vitest": "^3.2.4"
  }
}